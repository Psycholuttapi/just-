 for (int i = 0; i < 4; i++) {
            for (int k = 0; k< 4; k++) {

                for (int j = 0; j < 3; j++) {

                    if (a[j][i] < a[j + 1][i]) {
                        int temp = a[j][i];
                        a[j][i] = a[j + 1][i];
                        a[j + 1][i] = temp;
                    }
                }
            }
        }